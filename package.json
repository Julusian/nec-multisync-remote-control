{
  "name": "nec-multisync-remote-control",
  "version": "0.0.1",
  "description": "Nec Multisync (eg P401, P554) remote control",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julusian/nec-multisync-remote-control.git"
  },
  "author": "Julian Waller <git@julusian.co.uk>",
  "bugs": {
    "url": "https://github.com/Julusian/nec-multisync-remote-control/issues"
  },
  "homepage": "https://github.com/Julusian/nec-multisync-remote-control",
  "scripts": {
    "build": "trash dist && yarn build:main",
    "build:main": "tsc -p tsconfig.build.json",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "changelog": "standard-version",
    "release": "yarn reset && yarn docs:publish && yarn changelog",
    "reset": "git clean -dfx && git reset --hard && yarn",
    "validate:dependencies": "yarn audit && yarn license-validate",
    "license-validate": "node-license-validator -p -d --allow-licenses MIT BSD BSD-3-Clause ISC Apache"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.MD",
    "CHANGELOG.MD"
  ],
  "dependencies": {
    "object-path": "^0.11.4",
    "underscore": "^1.10.2"
  },
  "devDependencies": {
    "@types/node": "^12.11.2",
    "@types/object-path": "^0.11.0",
    "@types/underscore": "^1.9.3",
    "node-license-validator": "^1.3.0",
    "prettier": "^2.0.5",
    "trash-cli": "^3.0.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.6.4"
  }
}
